#include <stdio.h>

void update(int *a,int *b){
    int sum;
    int sub;
    sum=*a + *b;
    if(*a>*b){
        sub=*a-*b;
    }
    else{
        sub=*b - *a;
    }
    *a = sum;
    *b = sub;
} 

    // Complete this function    

int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    printf("%d\n%d", a, b);

    return 0;
}
