case 1:

#include<stdio.h>
void main()
{
	int i,j;
	char ch='A';
	char ch1='a';
	for(i=1;i<=5;i++)
	{
		for(j=1;j<=i;j++)
		{
			printf("%c%c\t",ch,ch1);
			ch++;
			ch1++;
		}
		printf("\n");
	}
}

case 2:

#include<stdio.h>
void main()
{
	int i,j;
	char ch='A';
	char ch1='a';
	for(i=1;i<=3;i++)
	{
		for(j=1;j<=i;j++)
		{
			printf("%c%c\t",ch,ch1);
			ch++;
			ch1++;
		}
		printf("\n");
	}
}
